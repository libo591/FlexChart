<config title='2010年产品销量' multiSeriesAxis="true">
    <axis>
        <x type="field" title='月份'/>
        <y id='a' type="linear" title='产量' beginValue="0" endValue="1000" inc="100"/>
        <y id='b' type="linear" title='销售额' beginValue="0" endValue="10000" inc="1000"/>
    </axis>
    <series>
        <line xField='label' yField='valueA' yAxis='a'/>
        <column xField='label' yField='valueB' yAxis='b'/>
    </series>
    <legend enable='true'/>
    <data>
